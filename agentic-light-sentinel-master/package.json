{
  "name": "infinity-loop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:watch": "concurrently \"pnpm dev\" \"pnpm watch:db\"",
    "worker": "tsx src/lib/exportWorker.ts",
    "dev:full": "concurrently \"pnpm dev\" \"pnpm watch:db\" \"pnpm worker\"",
    "insights:seed": "tsx prisma/seed-insights.ts",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start -p 3000",
    "start:prod": "cross-env NODE_ENV=production next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "prisma:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "watch:db": "nodemon --watch prisma/schema.prisma --exec \"pnpm db:generate\"",
    "seed": "tsx prisma/seed.ts",
    "seed:insights": "tsx prisma/seed-insights.ts",
    "seed:all": "pnpm seed && pnpm seed:insights",
    "import:geo": "tsx scripts/import-geo.ts",
    "ingest": "tsx scripts/ingest-viirs.ts",
    "ingest:daily": "tsx scripts/ingest-viirs-daily.ts",
    "hotspots": "tsx scripts/process-hotspots.ts",
    "notify": "tsx scripts/notify.ts",
    "notify:improved": "tsx scripts/notify-improved.ts",
    "debug:email": "tsx scripts/debug-email.ts",
    "email:test": "tsx scripts/quick-email-test.ts",
    "email:smtp": "tsx scripts/minimal-smtp-test.ts",
    "process:tiles": "tsx scripts/process-tiles.ts",
    "download:viirs": "tsx scripts/download-viirs.ts",
    "process:all": "pnpm download:viirs && pnpm process:tiles",
    "data:import": "pnpm import:geo && pnpm ingest",
    "data:process": "pnpm hotspots && pnpm process:tiles",
    "data:pipeline": "pnpm data:import && pnpm data:process",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:email": "vitest --run tests/email-alerts.test.ts",
    "test:watch": "vitest --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next dist coverage",
    "clean:deps": "rimraf node_modules pnpm-lock.yaml && pnpm install",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "deploy:build": "pnpm clean && pnpm install && pnpm build",
    "health": "tsx scripts/health-check.ts",
    "backup:db": "tsx scripts/backup-database.ts",
    "logs": "tsx scripts/view-logs.ts"
  },
